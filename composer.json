{
    "name": "antoniokadid/wappkit-core-integrations-slack",
    "description": "A PHP library to integrate with Slack.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Antonio Kadid",
            "email": "antoniokadid@gmail.com",
            "homepage": "http://www.antoniokadid.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1",
        "ext-json": "*",
        "antoniokadid/wappkit-core-collections": "dev-master@dev",
        "antoniokadid/wappkit-core-text": "dev-master@dev",
        "antoniokadid/wappkit-core-http": "dev-master@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "AntonioKadid\\WAPPKitCore\\Integrations\\Slack\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AntonioKadid\\WAPPKitCore\\Integrations\\Slack\\": "src/",
            "AntonioKadid\\WAPPKitCore\\Tests\\Integrations\\Slack\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --testdox tests"
    }
}
